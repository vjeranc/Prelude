{
    "parser": "babel-eslint",
    "ecmaFeatures": {
        "jsx": true
    },
    "extends": "airbnb",
    "plugins": [
        "react",
        "jsx-a11y",
        "import"
    ],
    "globals": {
        "window": false,
        "localStorage": false,
        "__BUILD__": false,
        "confirm": false,
        "navigator": false,
        "document": false
    }
}
